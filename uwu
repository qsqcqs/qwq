#!/bin/python

import re
import sys
mrow=0
if len(sys.argv) > 1:
	mrow=sys.argv[1:]			
	nya=mrow[0]
	mrow=mrow[1:]
	while len(mrow) > 0:
		nya=nya+' '+mrow[0]
		mrow=mrow[1:]		

	nya=re.sub("rl","#@!w#@!",nya)
	nya=re.sub("RL","#@!W#@!",nya)
	nya=re.sub("r","#@!w#@!",nya)
	nya=re.sub("l","#@!w#@!",nya)
	nya=re.sub("R","#@!W#@!",nya)
	nya=re.sub("L","#@!W#@!",nya)
	nya=re.sub("ow","#@!a#@!o#@!",nya)
	nya=re.sub("OW","#@!A#@!O#@!",nya)
	nya=re.sub("tha","#@!n#@!y#@!a#@!",nya)
	nya=re.sub("THA","#@!N#@!Y#@!A#@!",nya)
	nya=re.sub("you're","#@!u#@!r#@!",nya)
	nya=re.sub("(?<!\b)ove\b","#@!u#@!v#@!",nya)
	nya=re.sub("(?<!\b)OVE\b","#@!U#@!V#@!",nya)
	nya=re.sub("n(?=[aeiouAEIOU])","#@!n#@!y#@!",nya)
	nya=re.sub("th\b","#@!f#@!",nya)
	nya=re.sub("this(?!\b)","#@!f#@!i#@!s#@!",nya)
	nya=re.sub("th","#@!d#@!",nya)
	nya=re.sub("N(?=[aeiouAEIOU])","#@!N#@!Y#@!",nya)
	nya=re.sub("TH\b","#@!F#@!",nya)
	nya=re.sub("THIS(?!\b)","#@!F#@!I#@!S#@!",nya)
	nya=re.sub("TH","#@!D#@!",nya)
	nya=re.sub("(?<![aeiouyYAEIOU])e[iya]","#@!y#@!e#@!i#@!",nya)
	nya=re.sub("(?<![aeiouyYAEIOU])ie","#@!y#@!i#@!e#@!",nya)
	nya=re.sub("(?<![aeiouyYAEIOU])es","#@!y#@!e#@!s#@!",nya)
	nya=re.sub("(?<![aeiouyYAEIOU])a[e]","#@!y#@!a#@!e#@!",nya)
	nya=re.sub("(?<![aeiouyYAEIOU])ai","#@!y#@!a#@!i#@!",nya)
	nya=re.sub("(?<![aeiouyYAEIOU])o[ou]","#@!y#@!u#@!",nya)
	nya=re.sub("(?<![aeiouyYAEIOU])oe","#@!y#@!o#@!",nya)
	nya=re.sub("(?<![aeiouyYAEIOU])u[eu]","#@!u#@!w#@!u#@!",nya)
	nya=re.sub("(?<![aeiouyYAEIOU])e[u]","#@!u#@!w#@!u#@!",nya)
	nya=re.sub("(?<![aeiouyYAEIOU])E[IYA]","#@!Y#@!E#@!I#@!",nya)
	nya=re.sub("(?<![aeiouyYAEIOU])IE","#@!Y#@!I#@!E#@!",nya)
	nya=re.sub("(?<![aeiouyYAEIOU])ES","#@!Y#@!E#@!S#@!",nya)
	nya=re.sub("(?<![aeiouyYAEIOU])A[E]","#@!Y#@!A#@!E#@!",nya)
	nya=re.sub("(?<![aeiouyYAEIOU])AI","#@!Y#@!A#@!I#@!",nya)
	nya=re.sub("(?<![aeiouyYAEIOU])O[OU]","#@!Y#@!u#@!",nya)
	nya=re.sub("(?<![aeiouyYAEIOU])OE","#@!Y#@!O#@!",nya)
	nya=re.sub("(?<![aeiouyYAEIOU])U[EU]","#@!U#@!W#@!U#@!",nya)
	nya=re.sub("(?<![aeiouyYAEIOU])E[U]","#@!U#@!W#@!U#@!",nya)
	nya=re.sub("o[uo]","#@!e#@!w#@!",nya)
	nya=re.sub("O[OU]","#@!E#@!W#@!",nya)
	nya=re.sub("#@!","",nya)
	print(nya)
	exit()

while mrow==0:
		try:
			nya=input()
			nya=re.sub("rl","#@!w#@!",nya)
			nya=re.sub("RL","#@!W#@!",nya)
			nya=re.sub("r","#@!w#@!",nya)
			nya=re.sub("l","#@!w#@!",nya)
			nya=re.sub("R","#@!W#@!",nya)
			nya=re.sub("L","#@!W#@!",nya)
			nya=re.sub("ow","#@!a#@!o#@!",nya)
			nya=re.sub("OW","#@!A#@!O#@!",nya)
			nya=re.sub("tha","#@!n#@!y#@!a#@!",nya)
			nya=re.sub("THA","#@!N#@!Y#@!A#@!",nya)
			nya=re.sub("you're","#@!u#@!r#@!",nya)
			nya=re.sub("(?<!\b)ove\b","#@!u#@!v#@!",nya)
			nya=re.sub("(?<!\b)OVE\b","#@!U#@!V#@!",nya)
			nya=re.sub("n(?=[aeiouAEIOU])","#@!n#@!y#@!",nya)
			nya=re.sub("th\b","#@!f#@!",nya)
			nya=re.sub("this(?!\b)","#@!f#@!i#@!s#@!",nya)
			nya=re.sub("th","#@!d#@!",nya)
			nya=re.sub("N(?=[aeiouAEIOU])","#@!N#@!Y#@!",nya)
			nya=re.sub("TH\b","#@!F#@!",nya)
			nya=re.sub("THIS(?!\b)","#@!F#@!I#@!S#@!",nya)
			nya=re.sub("TH","#@!D#@!",nya)
			nya=re.sub("(?<![aeiouyYAEIOU])e[iya]","#@!y#@!e#@!i#@!",nya)
			nya=re.sub("(?<![aeiouyYAEIOU])ie","#@!y#@!i#@!e#@!",nya)
			nya=re.sub("(?<![aeiouyYAEIOU])es","#@!y#@!e#@!s#@!",nya)
			nya=re.sub("(?<![aeiouyYAEIOU])a[e]","#@!y#@!a#@!e#@!",nya)
			nya=re.sub("(?<![aeiouyYAEIOU])ai","#@!y#@!a#@!i#@!",nya)
			nya=re.sub("(?<![aeiouyYAEIOU])o[ou]","#@!y#@!u#@!",nya)
			nya=re.sub("(?<![aeiouyYAEIOU])oe","#@!y#@!o#@!",nya)
			nya=re.sub("(?<![aeiouyYAEIOU])u[eu]","#@!u#@!w#@!u#@!",nya)
			nya=re.sub("(?<![aeiouyYAEIOU])e[u]","#@!u#@!w#@!u#@!",nya)
			nya=re.sub("(?<![aeiouyYAEIOU])E[IYA]","#@!Y#@!E#@!I#@!",nya)
			nya=re.sub("(?<![aeiouyYAEIOU])IE","#@!Y#@!I#@!E#@!",nya)
			nya=re.sub("(?<![aeiouyYAEIOU])ES","#@!Y#@!E#@!S#@!",nya)
			nya=re.sub("(?<![aeiouyYAEIOU])A[E]","#@!Y#@!A#@!E#@!",nya)
			nya=re.sub("(?<![aeiouyYAEIOU])AI","#@!Y#@!A#@!I#@!",nya)
			nya=re.sub("(?<![aeiouyYAEIOU])O[OU]","#@!Y#@!u#@!",nya)
			nya=re.sub("(?<![aeiouyYAEIOU])OE","#@!Y#@!O#@!",nya)
			nya=re.sub("(?<![aeiouyYAEIOU])U[EU]","#@!U#@!W#@!U#@!",nya)
			nya=re.sub("(?<![aeiouyYAEIOU])E[U]","#@!U#@!W#@!U#@!",nya)
			nya=re.sub("o[uo]","#@!e#@!w#@!",nya)
			nya=re.sub("O[OU]","#@!E#@!W#@!",nya)
			nya=re.sub("#@!","",nya)
			print(nya)
		except EOFError:
			mrow=1
